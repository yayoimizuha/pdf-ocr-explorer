<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PDF_OCR_Explorer.SearchView">
    <ContentPage.Content>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="128"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            <Label Grid.Row="0" x:Name="Header" FontAttributes="Bold" FontSize="60"></Label>
            <Grid Grid.Row="1" x:Name="View"></Grid>
        </Grid>
    </ContentPage.Content>
</ContentPage>